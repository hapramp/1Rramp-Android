<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FeedListView">
        <attr name="wantTopSpaceOffset" format="boolean" />
        <attr name="topOffset" format="integer" />
        <attr name="refreshStart" format="integer"/>
        <attr name="refreshEnd" format="integer"/>
        <attr name="wantBottomSpaceOffset" format="boolean" />
        <attr name="bottomOffset" format="integer" />
    </declare-styleable>

    <declare-styleable name="CustomProgressBar">
        <attr name="primaryColor" format="string"/>
        <attr name="secondaryColor" format="string"/>
        <attr name="progressHeight" format="integer"/>
    </declare-styleable>
</resources>