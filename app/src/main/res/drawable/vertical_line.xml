<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="90"
    android:toDegrees="90">
    <shape android:shape="line">
        <size
            android:width="96dp"
            android:height="96dp" />
        <stroke
            android:width="2dp"
            android:color="#1b519a"
            android:dashGap="6dp"
            android:dashWidth="2dp" />
    </shape>
</rotate>